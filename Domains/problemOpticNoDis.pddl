(define (problem turtlebot)
(:domain MoBAr-Rover)
(:objects
c10_2 c5_9 c6_11 c6_10  - loc
  Leader Follower0 - Agent
	N_0 N_1 - NavMode
)
(:init
(= (distance_to_move c6_10 c6_11) 1.00000)
(= (distance_to_move c6_10 c5_9) 1.41421)
(= (distance_to_move c6_11 c6_10) 1.00000)
(= (distance_to_move c6_11 c5_9) 2.41421)
